{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package": "electron-packager . autoPunchIn –platform=win32 –arch=x64 –version=1.7.10–icon=icons/Mushroom.ico –asar=true –out=build –ignore=node_modules/electron-* "
  },
  "build": {
    "extraResources": [
      {
        "from": "./.env",
        "to": ".env"
      }
    ]
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^30.1.0",
    "electron-builder": "^24.13.3",
    "electron-packager": "^17.1.2"
  },
  "dependencies": {
    "dotenv": "^16.4.5"
  }
}
